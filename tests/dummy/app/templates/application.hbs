<div class="container-fluid">

  <h2 id="title">Ember Highcharts</h2>

  <section>
    <h3>Basic line chart</h3>
    {{chart-basic-line}}
  </section>

  <section>
    <h3>Basic bar chart with custom theme</h3>
    {{chart-basic-bar}}
  </section>

  <section>
    <h3>Basic highmaps chart</h3>
    {{chart-basic-map}}
  </section>

  <section>
    <h3>Column chart with drilldown</h3>
    {{chart-column-drilldown}}
  </section>

  <section>
    <h3>Solid gauge chart</h3>
    {{chart-solid-gauge}}
  </section>

  <section>
    <h3>Interactive highstock chart</h3>
    {{chart-highstock-interactive}}
  </section>

  <section>
    <h3>Interactive line chart</h3>
    {{chart-line-interactive}}
  </section>

  <section>
    <h3>Sales funnel chart</h3>
    {{chart-funnel}}
  </section>

  <section>
    <h3>Heatmap chart</h3>
    {{chart-heat-map}}
  </section>

</div>
